{
  "allowCompoundWords": true,
  "enableFiletypes": ["*"],
  "enableGlobDot": true,
  "ignorePaths": [
    ".git",
    ".gitignore",
    ".vscode",
    "pnpm-lock.yaml",
    "*.sql",
    "patches",
    ".prettierignore",
    ".env.*",
    ".env",
    ".eslintignore",
    ".eslintrc.*",
    "tsup.config.*",
    "tsconfig.json",
    "package.json",
    "cspell.config.json",
    "README.md"
  ],
  "numSuggestions": 0,
  "useGitignore": true,
  "words": [
    "Jiti",
    "xsmall",
    "xxsmall",
    "xlarge",
    "xxlarge",
    "SNADBOX",
    "zustand",
    "yxxx",
    "safy",
    "ianvs",
    "Turborepo",
    "TSCONVERSION",
    "topo",
    "tsbuildinfo"
  ]
}
