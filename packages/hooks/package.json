{
  "name": "@template/react-hooks",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "module": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:libs": "tsup",
    "clean": "rimraf .turbo node_modules dist",
    "dev": "tsup --watch",
    "fix:eslint": "eslint --max-warnings 0 --fix .",
    "lint": "eslint --max-warnings 0 .",
    "lint:typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@template/react": "workspace:*"
  },
  "devDependencies": {
    "@template/lintconfig": "workspace:*",
    "@template/tsconfig": "workspace:*",
    "@types/node": "^20.11.27",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
