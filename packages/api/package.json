{
  "name": "@template/api",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "module": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:packages": "tsup",
    "clean": "rimraf .turbo node_modules dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@tooling/prettier-config/base",
  "dependencies": {
    "destr": "^2.0.3",
    "ufo": "^1.5.3"
  },
  "devDependencies": {
    "@tooling/eslint-config": "workspace:*",
    "@tooling/prettier-config": "workspace:*",
    "@tooling/tsconfig": "workspace:*",
    "eslint": "^9.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "typescript": "^5.4.4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@8.12.1",
  "engines": {
    "bun": ">=1.0",
    "node": ">=18.0"
  }
}
