{
  "name": "@template/react",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./assertion": {
      "import": {
        "types": "./dist/assertion.d.ts",
        "default": "./dist/assertion.js"
      },
      "require": {
        "types": "./dist/assertion.d.cts",
        "default": "./dist/assertion.cjs"
      }
    },
    "./render": {
      "import": {
        "types": "./dist/render.d.ts",
        "default": "./dist/render.js"
      },
      "require": {
        "types": "./dist/render.d.cts",
        "default": "./dist/render.cjs"
      }
    },
    "./depsAreSame": {
      "import": {
        "types": "./dist/depsAreSame.d.ts",
        "default": "./dist/depsAreSame.js"
      },
      "require": {
        "types": "./dist/depsAreSame.d.cts",
        "default": "./dist/depsAreSame.cjs"
      }
    },
    "./dom": {
      "import": {
        "types": "./dist/dom.d.ts",
        "default": "./dist/dom.js"
      },
      "require": {
        "types": "./dist/dom.d.cts",
        "default": "./dist/dom.cjs"
      }
    },
    "./form": {
      "import": {
        "types": "./dist/form.d.ts",
        "default": "./dist/form.js"
      },
      "require": {
        "types": "./dist/form.d.cts",
        "default": "./dist/form.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:packages": "tsup",
    "clean": "rimraf .turbo node_modules dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@tooling/prettier-config/web",
  "devDependencies": {
    "@tooling/eslint-config": "workspace:*",
    "@tooling/prettier-config": "workspace:*",
    "@tooling/tsconfig": "workspace:*",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.4.5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.0.5",
  "engines": {
    "bun": ">=1.0",
    "node": ">=18.0"
  }
}
