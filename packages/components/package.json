{
  "name": "@template/react-components",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./client-only": {
      "import": {
        "types": "./dist/client-only.d.ts",
        "default": "./dist/client-only.js"
      },
      "require": {
        "types": "./dist/client-only.d.cts",
        "default": "./dist/client-only.cjs"
      }
    },
    "./error-boundary": {
      "import": {
        "types": "./dist/error-boundary.d.ts",
        "default": "./dist/error-boundary.js"
      },
      "require": {
        "types": "./dist/error-boundary.d.cts",
        "default": "./dist/error-boundary.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:packages": "tsup",
    "clean": "rimraf .turbo node_modules dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@tooling/prettier-config/web",
  "dependencies": {
    "@template/react": "workspace:*",
    "@template/react-hooks": "workspace:*"
  },
  "devDependencies": {
    "@tooling/eslint-config": "workspace:*",
    "@tooling/prettier-config": "workspace:*",
    "@tooling/tsconfig": "workspace:*",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0 ",
    "eslint": "^9.2.0",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "typescript": "^5.4.5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.0.5",
  "engines": {
    "bun": ">=1.0",
    "node": ">=18.0"
  }
}
