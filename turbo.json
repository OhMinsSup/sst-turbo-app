{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "globalEnv": ["NODE_ENV"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "build:libs": {
      "dependsOn": ["^build:libs"]
    },
    "build:env": {
      "dependsOn": ["^build:env"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "fix:eslint": {
      "dependsOn": ["^fix:eslint"],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"],
      "cache": false
    },
    "lint:typecheck": {
      "dependsOn": ["^lint:typecheck"],
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
